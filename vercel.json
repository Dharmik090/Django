{
    "builds": [
        {
            "src": "city_travel_management_2/wsgi.py",
            "use": "@vercel/python",
            "config": {"maxLambdaSize": "15mb", "runtime": "python3.12.2"}
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "city_travel_management_2/wsgi.py"
        }
    ]
}